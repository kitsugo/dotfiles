		},
		{
			"command": {
				"action": "splitPane",
				"split": "auto",
				"splitMode": "duplicate"
			},
			"keys": "alt+shift+d"
		},
		{
			"command": "unbound",
			"keys": "ctrl+tab"
		},
		{
			"command": "unbound",
			"keys": "ctrl+shift+tab"
		}
